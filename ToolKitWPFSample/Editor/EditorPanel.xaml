<Window x:Class="ToolKit.WPF.Sample.EditorPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ToolKit.WPF.Sample"
        xmlns:toolwpf="clr-namespace:ToolKit.WPF.Controls;assembly=ToolKit.WPF"
        mc:Ignorable="d"
        Title="EditorPanel" Height="450" Width="800">
    
    <Window.DataContext>
        <local:EditorPanelViewModel />
    </Window.DataContext>
    
    <toolwpf:DynamicTableGrid ItemsSource="{Binding Table}" />
</Window>
