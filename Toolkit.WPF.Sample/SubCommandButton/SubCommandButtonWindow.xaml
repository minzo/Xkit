<Window x:Class="Toolkit.WPF.Sample.SubCommandButtonWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Toolkit.WPF.Sample"
        xmlns:ctrls="clr-namespace:Toolkit.WPF.Controls;assembly=Toolkit.WPF"
        mc:Ignorable="d"
        Title="SubCommandButtonWindow" Height="450" Width="800">
    <StackPanel>
        <ctrls:SubCommandButton>
            <ctrls:SubCommandButton.SubCommandMenuItem>
                <MenuItem Header="HOGE" />
                <MenuItem Header="FUGA" />
            </ctrls:SubCommandButton.SubCommandMenuItem>
        </ctrls:SubCommandButton>
    </StackPanel>

</Window>
